{
  "name": "glean-developer-site-revamp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --host 127.0.0.1 --port 8888",
    "build": "yarn generate:changelog && yarn generate:rss && docusaurus build",
    "build:fast": "GENERATE_API_DOCS=false yarn generate:changelog && yarn generate:rss && docusaurus build",
    "start:fast": "GENERATE_API_DOCS=false docusaurus start --host 127.0.0.1 --port 8888",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "changelog:new": "node scripts/create-changelog-entry.mjs",
    "generate:changelog": "node scripts/generate-changelog-data.mjs",
    "generate:rss": "node scripts/generate-rss-feed.mjs",
    "generate:redirects": "tsx scripts/generate-redirects.ts",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "openapi:regenerate:all": "yarn openapi:clean:before:all && yarn openapi:transform:all && yarn openapi:generate:all && yarn openapi:clean:after:all",
    "openapi:clean:before:all": "yarn openapi:clean:before:client && yarn openapi:clean:before:indexing",
    "openapi:clean:before:client": "find docs/api/client-api -type f ! -name 'overview.mdx' -delete",
    "openapi:clean:before:indexing": "find docs/api/indexing-api -type f ! -name '*-overview.mdx' -delete",
    "openapi:transform:all": "yarn openapi:transform:client && yarn openapi:transform:indexing",
    "openapi:transform:client": "yarn openapi:capitalize:client && yarn openapi:break:client",
    "openapi:capitalize:client": "node scripts/openapi-capitalize-language.mjs https://gleanwork.github.io/open-api/specs/final/client_rest.yaml openapi/client/client-capitalized.yaml",
    "openapi:break:client": "node scripts/openapi-break-circular.mjs openapi/client/client-capitalized.yaml openapi/client/split-apis",
    "openapi:transform:indexing": "node scripts/openapi-capitalize-language.mjs https://gleanwork.github.io/open-api/specs/final/indexing.yaml openapi/indexing/indexing-capitalized.yaml",
    "openapi:generate:all": "yarn openapi:generate:client && yarn openapi:generate:indexing",
    "openapi:generate:client": "yarn openapi:generate:activity && yarn openapi:generate:announcements && yarn openapi:generate:answers && yarn openapi:generate:authentication && yarn openapi:generate:chat && yarn openapi:generate:agents && yarn openapi:generate:collections && yarn openapi:generate:documents && yarn openapi:generate:insights && yarn openapi:generate:messages && yarn openapi:generate:pins && yarn openapi:generate:search && yarn openapi:generate:entities && yarn openapi:generate:shortcuts && yarn openapi:generate:summarize && yarn openapi:generate:verification && yarn openapi:generate:tools && yarn openapi:generate:governance",
    "openapi:generate:activity": "yarn docusaurus gen-api-docs activity",
    "openapi:generate:announcements": "yarn docusaurus gen-api-docs announcements",
    "openapi:generate:answers": "yarn docusaurus gen-api-docs answers",
    "openapi:generate:authentication": "yarn docusaurus gen-api-docs authentication",
    "openapi:generate:chat": "yarn docusaurus gen-api-docs chat",
    "openapi:generate:agents": "yarn docusaurus gen-api-docs agents",
    "openapi:generate:collections": "yarn docusaurus gen-api-docs collections",
    "openapi:generate:documents": "yarn docusaurus gen-api-docs documents",
    "openapi:generate:insights": "yarn docusaurus gen-api-docs insights",
    "openapi:generate:messages": "yarn docusaurus gen-api-docs messages",
    "openapi:generate:pins": "yarn docusaurus gen-api-docs pins",
    "openapi:generate:search": "yarn docusaurus gen-api-docs search",
    "openapi:generate:entities": "yarn docusaurus gen-api-docs entities",
    "openapi:generate:shortcuts": "yarn docusaurus gen-api-docs shortcuts",
    "openapi:generate:summarize": "yarn docusaurus gen-api-docs summarize",
    "openapi:generate:verification": "yarn docusaurus gen-api-docs verification",
    "openapi:generate:tools": "yarn docusaurus gen-api-docs tools",
    "openapi:generate:governance": "yarn docusaurus gen-api-docs governance",
    "openapi:generate:indexing": "yarn docusaurus gen-api-docs indexing",
    "openapi:clean:after:all": "yarn openapi:clean:after:client && yarn openapi:clean:after:indexing",
    "openapi:clean:after:client": "find docs/api/client-api -type f -name 'sidebar.ts' -delete && find docs/api/client-api -type f -name '*.info.mdx' -delete && find docs/api/client-api -type f -name '*.tag.mdx' -delete",
    "openapi:clean:after:indexing": "rm -f docs/api/indexing-api/sidebar.ts docs/api/indexing-api/glean-api.info.mdx && find docs/api/indexing-api/ -type f -name '*.tag.mdx' -delete && find docs/api/indexing-api/ -type f -name '*.info.mdx' -delete"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/plugin-client-redirects": "^3.8.1",
    "@docusaurus/plugin-content-docs": "^3.8.1",
    "@docusaurus/plugin-vercel-analytics": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-classic": "^3.8.1",
    "@docusaurus/theme-common": "^3.8.1",
    "@docusaurus/theme-search-algolia": "^3.8.1",
    "@docusaurus/utils": "^3.8.1",
    "@docusaurus/utils-validation": "^3.8.1",
    "@mdx-js/react": "^3.0.0",
    "@signalwire/docusaurus-plugin-llms-txt": "^1.1.0",
    "@types/node": "^24.0.13",
    "clsx": "^2.1.1",
    "docusaurus-plugin-openapi-docs": "^4.4.0",
    "docusaurus-plugin-search-glean": "^0.5.6",
    "docusaurus-theme-openapi-docs": "^4.4.0",
    "feed": "^5.1.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.513.0",
    "marked": "^15.0.12",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10",
    "swiper": "^11.2.8",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@docusaurus/faster": "^3.8.1",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/xml2js": "^0.4.14",
    "chalk": "^5.4.1",
    "fast-levenshtein": "^3.0.0",
    "inquirer": "^12.6.3",
    "open": "^10.1.2",
    "prettier": "^3.6.1",
    "sass": "^1.89.1",
    "sass-loader": "^16.0.5",
    "slugify": "^1.6.6",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "xml2js": "^0.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=22.0"
  }
}
